{
  "name": "vite-boilerplate",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "typecheck": "tsc --noEmit",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --fix",
    "preview": "vite preview",
    "format": "prettier . --config .prettierrc --ignore-path .prettierignore --write '**/*.{css,scss,cjs,mjs,ts,tsx}'",
    "format:check": "prettier . --config '.prettierrc' --check  --ignore-path '.prettierignore'",
    "prepare": "husky install"
  },
  "dependencies": {
    "@lukemorales/query-key-factory": "^1.3.4",
    "@orbs-network/ton-access": "^2.3.3",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@rollup/plugin-alias": "^5.1.0",
    "@t3-oss/env-core": "^0.9.2",
    "@tact-lang/emulator": "^4.2.3",
    "@tanstack/react-query": "^5.28.9",
    "@tanstack/react-query-devtools": "^5.28.10",
    "@tanstack/react-router": "^1.34.7",
    "@tanstack/router-devtools": "^1.26.1",
    "@vitejs/plugin-basic-ssl": "^1.0.1",
    "axios": "^1.6.8",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwind-merge": "^2.3.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@tanstack/router-vite-plugin": "^1.25.0",
    "@types/node": "^20.12.2",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "postcss": "^8.4.38",
    "prettier": "3.2.5",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vite-tsconfig-paths": "^4.3.2",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.5"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,cjs,css,mjs}": [
      "prettier --write"
    ]
  }
}
